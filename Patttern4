#include <stdio.h>

int main()
{
    int n;
    scanf("%d",&n);
    int i=n;
    while(i>=1){
        int j=n-1;
        while(j>=n-i){
            printf(" ");
            j=j-1;
        }
        int k=n;
        while(k>=i){
            printf("%d",k);
            k=k-1;
            
        }
        printf("\n");
         i=i-1;
    }
   
    }
